{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ng5"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/theme-assets/css/lib/chartist/chartist.min.css",
        "assets/theme-assets/css/lib/font-awesome.min.css",
        "assets/theme-assets/css/lib/themify-icons.css",
        "assets/theme-assets/css/lib/owl.carousel.min.css",
        "assets/theme-assets/css/lib/owl.theme.default.min.css",
        "assets/theme-assets/css/lib/weather-icons.css",
        "assets/theme-assets/css/lib/menubar/sidebar.css",
        "assets/theme-assets/css/lib/unix.css",
        "assets/theme-assets/css/lib/bootstrap.min.css",
        "assets/theme-assets/css/style.css"
      ],
      "scripts": [
        { "input": "assets/theme-assets/js/lib/jquery.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/jquery.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/jquery.nanoscroller.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/menubar/sidebar.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/preloader/pace.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/bootstrap.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/weather/jquery.simpleWeather.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/weather/weather-init.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/circle-progress/circle-progress.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/circle-progress/circle-progress-init.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/chartist/chartist.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/chartist/chartist-init.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/sparklinechart/jquery.sparkline.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/sparklinechart/sparkline.init.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/owl-carousel/owl.carousel.min.js", "lazy": true },
        { "input": "assets/theme-assets/js/lib/owl-carousel/owl.carousel-init.js", "lazy": true },
        { "input": "assets/theme-assets/js/scripts.js", "lazy": true }
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
